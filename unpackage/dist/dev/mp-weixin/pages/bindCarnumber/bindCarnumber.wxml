<view><view class="section-cont1"><view class="header">车辆信息</view><view class="bodyer"><view class="row1"><view data-event-opts="{{[['tap',[['showProvince',['$event']]]]]}}" class="province" catchtap="__e">{{province}}</view><input placeholder="请输入车牌号" placeholder-class="placeholder" focus="{{true}}" data-event-opts="{{[['input',[['setCarNumber',['$event']]]]]}}" value="{{carNumber}}" bindinput="__e"/></view><view class="row2">手机号</view><view class="row4"><input class="input phonenumber" disabled="{{true}}" type="number" data-event-opts="{{[['input',[['setPhoneNumber',['$event']]]]]}}" value="{{phoneNumber}}" bindinput="__e"/></view><view class="row3"></view></view></view><block wx:if="{{!isedit}}"><view class="btns"><block wx:if="{{!isjihuo}}"><view data-event-opts="{{[['tap',[['save',['$event']]]]]}}" class="btn" catchtap="__e">确认生成</view></block><block wx:if="{{isjihuo}}"><view data-event-opts="{{[['tap',[['saveJihuo',['$event']]]]]}}" class="btn" catchtap="__e">激活挪车码</view></block></view></block><block wx:if="{{isedit}}"><view class="btns"><block wx:if="{{isedit}}"><view data-event-opts="{{[['tap',[['doedit',['$event']]]]]}}" class="{{[btnDisabled?'disabled':'disableds']}}" catchtap="__e">保存</view></block></view></block><block wx:if="{{show}}"><view class="province-action"><view class="bodyer"><block wx:for="{{provincename}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list" hover-class="hover-bgcolor" data-province="{{item}}" data-event-opts="{{[['tap',[['selectProvince',['$event']]]]]}}" catchtap="__e">{{''+item+''}}</view></block><text data-event-opts="{{[['tap',[['hideProvince',['$event']]]]]}}" class="list list-hide" catchtap="__e">隐藏</text></view></view></block></view>