<view><block wx:for="{{codeList}}" wx:for-item="item" wx:for-index="index" wx:key="unique"><view class="cont-list" data-carcodeid="{{item.id}}" data-event-opts="{{[['tap',[['linkExperienceCode',['$event']]]]]}}" catchtap="__e"><view class="list"><view class="col-1"><image mode="widthFix" src="{{item.imgurl}}"></image></view><view class="col-2"><view class="row1"><text class="s1">{{item.car_number}}</text><block wx:if="{{item.is_test==1}}"><text class="s2">体验码</text></block><block wx:if="{{item.is_test==0}}"><text class="s2">正式码</text></block><block wx:if="{{item.is_test==3}}"><text class="s2">未激活码</text></block></view><view class="row2">{{"绑定手机： "+item.phone_number}}</view></view><view class="col-3"><image mode="widthFix" src="/static/pages/images/icon-arrow1.png"></image></view></view></view></block><view class="{{['nodata '+(nodata?'show':'')]}}">您还没有挪车码！</view></view>